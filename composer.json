{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "carbondate/carbon": "^1.23",
        "guzzlehttp/guzzle": "^6.3",
        "ifsnop/mysqldump-php": "^2.3",
        "league/flysystem": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "lookyman/phpstan-symfony": "^0.5.0",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^0.9.2",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/dotenv": "^4.0",
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpcs --standard=PSR2 --colors -pn src/",
            "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming --suffixes php",
            "vendor/bin/phpstan analyse --level max -c phpstan.neon src"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C7TRBQ0STMCQJ21G5GTXDVPK",
            "allow-contrib": false
        }
    }
}
